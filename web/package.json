{
  "name": "wikijump",
  "private": true,
  "license": "agpl-3.0-or-later",
  "scripts": {
    "build": "run-p build:legacy build:local",
    "build:legacy": "node scripts/build-legacy.js build",
    "build:local": "pnpm compose build",
    "compose": "cd ../install/local/dev && docker-compose -p wikijump -f docker-compose.yaml -f docker-compose.dev.yaml",
    "dev": "pnpm build:local && run-p dev:legacy dev:local",
    "dev:legacy": "node scripts/build-legacy.js dev",
    "dev:local": "pnpm compose up",
    "nabs": "nabs"
  },
  "browserslist": [
    "supports es6-module and > 0.2% and last 2 versions",
    "Firefox ESR",
    "not dead",
    "not ie > 0"
  ],
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@typescript-eslint/typescript-estree": "^4.32.0",
    "esbuild": "^0.13.3",
    "eslint": "^7.32.0",
    "eslint-plugin-regexp": "^1.3.1",
    "eslint-plugin-svelte3": "^3.2.1",
    "eslint-plugin-tsdoc": "^0.2.14",
    "nabs": "^0.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-plugin-jsdoc": "^0.3.24",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-svelte": "^2.4.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "svelte": "^3.43.0",
    "typescript": "^4.4.3",
    "vite": "2.5.10"
  },
  "dependencies": {
    "compress-tag": "^2.0.0",
    "csstype": "^3.0.8",
    "query-string": "^6.14.1"
  },
  "engineStrict": true,
  "engines": {
    "pnpm": "*",
    "yarn": "forbidden, use pnpm",
    "npm": "forbidden, use pnpm"
  }
}
