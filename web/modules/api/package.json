{
  "name": "@wikijump/api",
  "license": "agpl-3.0-or-later",
  "description": "API client for Wikijump.",
  "version": "0.0.0",
  "keywords": [
    "wikijump"
  ],
  "private": true,
  "scripts": {
    "generate-api": "swagger-typescript-api -p ../../../web/resources/api/api.oas3.yaml -o ./vendor --name api.ts --unwrap-response-data  --module-name-index -1 --clean-output",
    "mock-start": "mockoon-cli start --data ./mockoon/mockoon-api.json --name \"Wikijump\"",
    "mock-stop": "mockoon-cli stop 0"
  },
  "type": "module",
  "main": "src/index.ts",
  "devDependencies": {
    "@mockoon/cli": "^1.5.0",
    "swagger-typescript-api": "^9.3.1"
  },
  "dependencies": {
    "@wikijump/util": "workspace:*",
    "svelte": "^3.44.3",
    "ziggy-js": "^1.4.2"
  }
}
