{
  "name": "components",
  "license": "agpl-3.0-or-later",
  "description": "Wikijump component library.",
  "version": "0.0.0",
  "keywords": [
    "wikijump"
  ],
  "scripts": {
    "clean": "trash dist .ultra.cache.json",
    "dev": "node ../scripts/build-components.js -w",
    "build:cmd": "tsc && node ../scripts/build-components.js",
    "build": "ultra build:cmd"
